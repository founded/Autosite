                    %nav->value%
                </ul>
            </nav>
            %menutypes->value% 
            %find->value%
            <div id="langbar" >
                <a data-toggle="collapse" data-target="#myNavbar,#flags" ><img class="icon" src="%path.layout->value%img/icons/m/menu.svg" alt="%collapse%" ></a>
            	<div id="flags" class="%collapse->value%" >
                    <a href="https://nl.%URL_SELF->value%" title="%site.name% %Dutch%" ><img src="%path.layout->value%img/FLAGS/flag_be.gif" alt="%Dutch%" ></a>
                    <a href="https://en.%URL_SELF->value%" title="%site.name% %english%" ><img src="%path.layout->value%img/FLAGS/flag_en.gif" alt="%english%" ></a>
                    <a href="https://fr.%URL_SELF->value%" title="%site.name% %french%" ><img src="%path.layout->value%img/FLAGS/flag_fr.gif" alt="%french%" ></a>
                    <a href="https://nl.%URL_SELF->value%" title="%site.name% %Dutch%" ><img src="%path.layout->value%img/FLAGS/flag_nl.gif" alt="%Dutch%" ></a>
                    <a href="https://es.%URL_SELF->value%" title="%site.name% %Spanish%" ><img src="%path.layout->value%img/FLAGS/flag_es.gif" alt="%Spanish%" ></a>
                    <a href="https://ru.%URL_SELF->value%" title="%site.name% %Russia%" ><img src="%path.layout->value%img/FLAGS/flag_ru.gif" alt="%Russia%" ></a>
                </div>
            </div>
            <svg id="boat" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" >
