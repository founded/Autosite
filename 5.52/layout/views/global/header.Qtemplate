            <defs>
              <path id="pathfortext" d="M 7 75 C 10 1 200 90 270 80 C 280 80 460 62 1480 70" fill="none" />
            </defs>
            <use xlink:href="#pathfortext" fill="none" stroke="none" />
            <text font-family="sans-serif" font-size="24" fill="#666699" >
              <textPath xlink:href="#pathfortext" startOffset="15" >
                  <tspan fill="#ffE" >%title->value%</tspan>
              </textPath>
            </text>
        </svg>
        <header id="tophead" >
            <nav id="myNavbar" class="%collapse->value%" >
