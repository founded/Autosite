<!--module -->
<div class="form-group" >
    <label class="control-label col-lg-4 col-md-4 col-sm-4" for="q" >%term%:</label>
    <input id="q" onclick="settextselectedfield('q')" class="text-right" type="text" name="q" id="q" value="%q->value%" required autofocus >
</div>
<div class="form-group" >
    <label class="control-label col-lg-4 col-md-4 col-sm-4" for="section" >%section%:</label>
    <select class="col-sm-3 text-right" name="section" size="1" > %sections->value% </select>
</div>